{
  "name": "git-overwrite-stash",
  "displayName": "Git overwrite stash",
  "description": "Adds a command to overwrite a Git stash",
  "version": "1.0.0",
  "publisher": "squiddy-code",
  "repository": {
    "type": "git",
    "url": "git://github.com/squiddy-code/git-overwrite-stash.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-overwrite-stash.overwriteStash",
        "title": "Overwrite a Git Stash",
        "category": "Git Overwrite Stash"
      }
    ]
  },
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  }
}
